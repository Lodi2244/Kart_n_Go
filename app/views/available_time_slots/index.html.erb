<p id="notice"><%= notice %></p>

<h1>Available Time Slots</h1>

<table>
  <thead>
    <tr>
      <th>Hour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timeslots.each do |timeslot| %>
      <tr>
        <td><%= timeslot.hour %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Available Time Slot', new_available_time_slot_path %>
